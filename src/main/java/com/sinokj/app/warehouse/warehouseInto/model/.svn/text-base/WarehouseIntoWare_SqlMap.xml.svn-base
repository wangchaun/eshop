	<!--进销存汇总模糊搜索 2-->
	<sql id="WarehouseIntoWare_discrepancy2">
		<dynamic prepend="where">		
			<isNotEmpty prepend="and" property="goodName">
                def.delivery_time>=#goodName#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="goodCode">
