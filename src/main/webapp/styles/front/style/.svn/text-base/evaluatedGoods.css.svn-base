/*============================产品栏目===================================*/

.photolist02{width:948px; float:left; overflow:hidden; padding-left:7px; padding-right:7px; padding-top:10px; padding-bottom:25px;}
.photolist02 li{width:237px; float:left; text-align:center; padding-top:15px; vertical-align:bottom;}
.photolist02 li img{width:215px; height:215px; border:#FF0000 solid 1px;}
.photolist02 li p{width:217px;  padding-top:10px; padding-left:10px; padding-right:10px; text-align:left; float:left; line-height:22px; }
.photolist02 li p a{ width:217px; display:block;}
.photolist02 li h1{width:217px; float:left; padding-left:10px; padding-top:3px; padding-right:10px; display:block; text-align:left;  line-height:22px; color:#e41215; font-size:18px;  font-family:"Arial";}
.photolist02 li h1 span{ line-height:22px; color:#999999; font-size:12px;  font-family:"宋体"; font-weight:normal; padding-left:10px;}
.photolist02 li h1 samp { line-height:14px; color:#999999; font-size:12px;  font-family:"宋体"; text-decoration:line-through; font-weight:normal; }
.photolist02 li h1 samp .font{  color:#999999; font-size:12px;  font-family:Arial, Helvetica, sans-serif; text-decoration:none;  font-weight:normal; margin:0px; padding:0px;}
.leftborder{ border-right:#efefef solid 1px;}
.photolist02 li h1 .font2{ color:#e41215; font-size:18px;  font-family:"Arial"; float:left;}

