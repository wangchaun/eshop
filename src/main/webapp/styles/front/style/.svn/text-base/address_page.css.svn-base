.sb {
    position:relative;
    height:24px;
>    height:24px;
    border:#d9d9d9 solid 1px;
    float:left;
    color:#666;	
}
.sb .b {/*模拟下被切除的下拉框(select)*/
    position:absolute;
    height:24px;
    line-height:28px;
    padding:0 5px;
    overflow:hidden;
    white-space :nowrap;text-overflow:ellipsis;
    font-family:verdana;
    font-size:12px;
}
.sb select {
    position: absolute;
    top:1px;
    right:1px;
}

.w_50{/*以此来控制该下拉框的长度，可按需要添加多个长度Class，如w_100，w_200*/
    width:109px !important;
}
.w_50 select{/*根据长度切割默认下拉框*/
    width:107px;
    clip:rect(2px 107px 24px 90px);/*切除下拉框(select)内容区域，只保留下拉三角箭头*/
	

}


/*兼容谷歌浏览器的*/
@media screen and (-webkit-min-device-pixel-ratio:0) {
.sb {
    position:relative;
    height:24px;
>    height:24px;
    border:#d9d9d9 solid 1px;
    float:left;
    color:#666;
	
}
.sb .b {/*模拟下被切除的下拉框(select)*/
    position:absolute;
    height:24px;
    line-height:24px;
    padding:0 5px;
    overflow:hidden;
    white-space :nowrap;text-overflow:ellipsis;
    font-family:verdana;
    font-size:12px;
}
.sb select {
    position: absolute;
    top:1px;
    right:3px;
}
}


