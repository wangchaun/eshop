@charset "utf-8";
/*Slide Start幻灯片位置开始*/
#Slidebox{width:100%; height:425px;  padding:0px;  margin:0 auto;  /*position:relative;*/ background:#fff; overflow:hidden;}
#flashBg {height: 425px;overflow: hidden;width:100%;clear: both;margin: 0;padding: 0;}
#flashLine {height: 425px;width: 100%;clear: both;}
#flash{width: 1200px;height: 425px;overflow: hidden;margin: 0 auto;clear: both;position: relative;padding: 0px 0px;}
#flash a {width: 1200px;height: 425px;display: none;}
#flash img {width: 1200px;height: 425px;}
.flash_bar {width:650px;padding-left: 530px;z-index: 69;height: 30px;overflow: hidden;position: absolute;padding-top: 13px!important;padding-top: 12px;
bottom:0px;left:0px; text-align: center;}
.flash_bar .dq {
display:block;float:left;width: 24px;height: 24px;margin:0px 6px;cursor: pointer;background: url(../../../Images/images/pagination2.png) no-repeat 0px 0px; overflow:hidden;
_background: none; 
_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/pagination2.png" ,sizingMethod="crop");
_position:0 0px;
}
.flash_bar .no {display: block;float: left;width:12px;height:12px;margin:6px 13px;cursor: pointer;
background:url(../../../Images/images/pagination.png) no-repeat 0px 0px; overflow:hidden;
_background: none;
_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/pagination.png" ,sizingMethod="crop");
}




