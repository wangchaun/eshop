/*refilter*/
#refilter {
	width:207px;
	border:2px solid #ddd;
}
#refilter .mt {
	height:36px;
	padding-left:8px;
	background-position:0 -97px;
	background-color:#F5F5F5;
	background-repeat:repeat-x;
	line-height:34px;
}
#refilter .mt h2, #refilter .mt h2 a {
	font-size:14px;
	color:#C80202;
}
#refilter .mc {
	width:207px;
}
#refilter .link {
	position:relative;
	padding:4px 4px 4px 8px;
	margin:0 2px 2px;
	background:#efefef;
	border-bottom:1px solid #ccc;
	font-weight:bold;
	zoom:1;
}
#refilter .link b {
	position:absolute;
	top:10px;
	left:4px;
	display:inline-block;
	width:6px;
	height:7px;
	background:url(../images/icon_psearch.png) no-repeat 0 0;
}
#refilter .item {
	position:relative;
	background:#fff;
}
#refilter .item h3 {
	overflow:hidden;
	height:24px;
	padding:3px 6px 3px 36px;
	border-top:1px dotted #ccc;
	background:#FAFAFA;
	line-height:24px;
	cursor:pointer;
}
#refilter .fore h3 {
	border:none;
}
#refilter .item b {
	position:absolute;
	top:7px;
	left:10px;
	width:16px;
	height:16px;
	margin-top:1px;
	background-position:-78px -339px;
	overflow:hidden;
	cursor:pointer;
}
#refilter .item ul {
	padding:4px 0 4px 18px;
	border-top:1px dotted #ccc;
	overflow:hidden;
	zoom:1;
	display:none;
}
#refilter .item li {
	height:24px;
	padding:0px 4px 0px 0;
	overflow:hidden;
}
#refilter .item span {
	font-size:13px;
	font-weight:normal;
	color:#999;
	font-family:arial;
}
#refilter .item li s {
	display:inline-block;
	overflow:hidden;
	width:13px;
	height:24px;
	margin-right:5px;
	background-position:-33px -339px;
	line-height:0;
	vertical-align:middle;
	font-size:0;
}
#refilter .item li s.tree-last {
	background-position:-47px -339px;
}
#refilter .item li a:hover, #refilter .item li a.curr:link, #refilter .item li a.curr:visited {
	background:#4598D2;
	color:#fff;
}
#refilter .item li a:hover, #refilter .item li a.curr:hover {
	text-decoration:none;
}
#refilter .hover b {
	background-position:-61px -339px;
}
#refilter .hover ul {
	display:block;
}
#refilter .extra {
	overflow:hidden;
	position:relative;
	z-index:1;
	height:38px;
	margin-top:-1px;
	border-top:1px dotted #ccc;
	background:#fafafa;
	line-height:38px;
}
#refilter .return {
	float:right;
	margin-right:5px;
}
#refilter .more {
	overflow:hidden;
	display:block;
	text-align:right;
	color:#333;
	font-size:12px;
}
#refilter .more span {
	display:inline-block;
	padding-right:20px;
	cursor:pointer;
}
#refilter .more b {
	display:inline-block;
	position:relative;
	top:1px;
	left:-15px;
	width:9px;
	height:11px;
	overflow:hidden;
*top:-4px;
}
#refilter .more b.open {
	background-position:-20px -14px;
}
#refilter .more b.close {
	background-position:-20px -26px;
}
