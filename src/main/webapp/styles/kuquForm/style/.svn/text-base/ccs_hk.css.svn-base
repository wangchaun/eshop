.hk_right2{float:right;width:90px;text-align:right;margin-top:0px;padding-right:5px;}

/* head style */
#hk_head {clear: both;width: 990px;margin: 0px auto;}
.hk_head_tipbox {margin-top: 4px;text-align: left; height:18px;}
	.hk_head_tiptime {background: url(../images/icon_head_01.gif) no-repeat;float: left;padding-left: 18px;margin: 2px 8px;}
	.hk_head_welcome {float: left;padding-left: 20px;margin: 2px 8px;background: url(../images/icon_head_02.gif) left 1px no-repeat;}
	.hk_head_list {float: right;margin: 5px 6px 2px 0px;padding: 0px;}
	.hk_head_list ul {list-style-type: none;margin: 0px;padding: 0px;}
	.hk_head_list li {float: left;margin: 0px 3px;}
	.hk_head_list li a {color:#585858;text-decoration:none;}
	.hk_head_list li a:visited{color:#585858;text-decoration:none;}
	.hk_head_list li a:hover{color:#ff5413;text-decoration:underline;}

.hk_head_banner {width: 990px;clear: both;background: url(../images/head_bg.jpg) no-repeat;height: 91px;margin: 3px auto 0px auto;}
	/* nav tab */
	.CCS_iconLick{float:right; margin-top:5px;*margin-top:20px; margin-right:18px;}
	.CCS_iconLick  li  {float:left;list-style-type: none;margin-left:3px;}
	.CCS_iconLick  a   {display:block; width:63px; height:57px; background:url(../images/index_17.jpg) no-repeat; }
	.CCS_iconLick  span{display:none;}
	  /* link  default */
	  .CCS_iconLick a.home    {background-position:0px 0px;}
	  .CCS_iconLick a.oa      {background-position:-63px 0px;}
	  .CCS_iconLick a.web     {background-position:-126px 0px;}
	  .CCS_iconLick a.mail    {background-position:-189px 0px;}
	  .CCS_iconLick a.sgs     {background-position:-252px 0px;}
	   /* link a*/
	  .CCS_iconLick li.c  a.home    {background-position:0px -57px;}
	  .CCS_iconLick li.c  a.oa      {background-position:-63px -57px;}
	  .CCS_iconLick li.c  a.web     {background-position:-126px -57px;}
	  .CCS_iconLick li.c  a.mail    {background-position:-189px -57px;}
	  .CCS_iconLick li.c  a.sgs     {background-position:-252px -57px;}
	   /*link hover*/
	  .CCS_iconLick a.home:hover    {background-position:0px -57px;}
	  .CCS_iconLick a.oa:hover      {background-position:-63px -57px;}
	  .CCS_iconLick a.web:hover      {background-position:-126px -57px;}
	  .CCS_iconLick a.mail:hover     {background-position:-189px -57px;}
	  .CCS_iconLick a.sgs:hover     {background-position:-252px -57px;}
	  
