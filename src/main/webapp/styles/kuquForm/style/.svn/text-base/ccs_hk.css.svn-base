@charset "utf-8";
/* EABOX产品样式 */
body {margin:0px auto; font-size:12px; font-family:"宋体","Arial"; color:#585858;}
img {border: none;}
ul,li{list-style-type: none;}
.hk_left{text-align:left;}
.hk_right{text-align:right;}
